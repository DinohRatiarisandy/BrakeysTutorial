;   param=value ; assign values to parameters

config_version=5

[application]

config/name="BrakeysTutorial"
run/main_scene="res://Scenes/game.tscn"
config/features=PackedStringArray("4.2", "C#", "GL Compatibility")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=768

[dotnet]

project/assembly_name="BrakeysTutorial"

[editor_plugins]

enabled=PackedStringArray("res://addons/kanban_tasks/plugin.cfg")

[layer_names]

2d_physics/layer_1="Player"
2d_physics/layer_2="Platform"
2d_physics/layer_3="Coin"
2d_physics/layer_4="KillZone"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
